{
  "title": "Remap Cmd+S to Ctrl+B",
  "rules": [
    {
      "description": "Cmd+S sends Ctrl+B",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": ["left_command"]
            }
          },
          "to": [
            {
              "key_code": "b",
              "modifiers": ["left_control"]
            }
          ]
        }
      ]
    }
  ]
}
